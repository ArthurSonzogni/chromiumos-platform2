# File to store firmware timing information
BIOS_TIMES_FILE="/var/log/bios_times.txt"

if ! /usr/bin/cbmem > ${BIOS_TIMES_FILE} 2> /dev/null; then
  echo "unavailable" > ${BIOS_TIMES_FILE}
fi

