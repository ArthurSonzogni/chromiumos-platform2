{
    "acVersion": "0.5.1",
    "acKind": "PodManifest",
    "apps": [
        {
            "name": "psyche-demo",
            "image": {
                "name": "com.android.embedded.demo-brick",
                "id": "sha512-...",
                "labels": [
                    {
                        "name":  "version",
                        "value": "1.0.0"
                    }
                ]
            },
            "app": {
                "exec": [
                    "/usr/bin/psyche_demo_server"
                ],
                "user": "chronos",
                "group": "chronos",
                "isolators": [
                    {
                        "name": "os/linux/capabilities-retain-set",
                        "value": {
                            "set": ["CAP_NET_BIND_SERVICE"]
                        }
                    }
                ]
            }
        }
    ],
    "annotations": [
        {
            "name": "bruteus-endpoint-0",
            "value": "com.android.embedded.demo-brick.psyche-demo-server"
        }
    ]
}
