{
    "acVersion": "0.5.1",
    "acKind": "PodManifest",
    "apps": [
        {
            "name": "ping_service",
            "image": {
                "name": "com.android.embedded.ping_brick",
                "id": "sha512-...",
                "labels": [
                    {
                        "name":  "version",
                        "value": "1.0.0"
                    }
                ]
            },
            "app": {
                "exec": [
                    "/usr/bin/ping_service",
                    "--quiet"
                ],
                "user": "100",
                "group": "300",
                "workingDirectory": "/opt/work",
                "isolators": [
                    {
                        "name": "os/brillo/namespaces-remove-set",
                        "value": {
                            "set": [ "CLONE_NEWNS " ],
                        }
                    },
                ],
                "ports": [
                    {
                        "name": "health",
                        "port": 4000,
                        "protocol": "tcp",
                        "socketActivated": true
                    }
                ]
            },
        },
    ],

}
