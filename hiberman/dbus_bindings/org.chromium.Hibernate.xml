<?xml version="1.0" encoding="UTF-8" ?>

<node name="/org/chromium/Hibernate"
  xmlns:tp="http://telepathy.freedesktop.org/wiki/DbusSpec#extensions-v0">
<interface name="org.chromium.HibernateSeedInterface">
  <!--
  SetSeedMaterial:
  @seed: Array of hibernate secret seed bytes.

  Gives the hibernate service its secret key material, based on user
  authentication data. This will be used to derive the public/private
  hibernate key pair used to protect the hibernate image. This seed should be
  at least 32 bytes.
  -->
  <method name="SetSeedMaterial">
    <arg name="seed" type="ay" direction="in" />
    <annotation name="org.chromium.DBus.Method.Kind" value="simple"/>
  </method>
</interface>
</node>
