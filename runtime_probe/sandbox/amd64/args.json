{
  "ap_i2c": {
    "binds": [
      {
        "dirname": "/dev",
        "basename": "i2c-\\d+"
      }
    ],
    "group": "i2c"
  },
  "ata_storage": {
    "binds": [
      "/sys/class/block/",
      "/sys/devices/pci0000:00/"
    ],
    "other_args": [
      "-k",
      "tmpfs,/run,tmpfs,MS_NODEV|MS_NOEXEC|MS_NOSUID,mode=755,size=10M",
      "-k",
      "tmpfs,/sys,tmpfs,MS_NODEV|MS_NOEXEC|MS_NOSUID,mode=755,size=10M"
    ]
  },
  "audio_codec": {
    "binds": [
      "/sys/kernel/debug/asoc/"
    ],
    "other_args": [
      "-k",
      "tmpfs,/sys,tmpfs,MS_NODEV|MS_NOEXEC|MS_NOSUID,mode=755,size=10M"
    ]
  },
  "cellular_network": {
    "binds": [
      "/run/dbus",
      "/sys/class/net/",
      "/sys/devices/pci0000:00/"
    ],
    "other_args": [
      "-k",
      "tmpfs,/run,tmpfs,MS_NODEV|MS_NOEXEC|MS_NOSUID,mode=755,size=10M",
      "-k",
      "tmpfs,/sys,tmpfs,MS_NODEV|MS_NOEXEC|MS_NOSUID,mode=755,size=10M"
    ]
  },
  "ec_i2c": {
