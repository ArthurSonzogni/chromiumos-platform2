mmap: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
mseal: 1
openat: 1
newfstatat: 1
close: 1
read: 1
mprotect: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
fstat: 1
fstatfs: 1
gettid: 1
write: 1
writev: 1
# CROS_EC_DEV_IOCXCMD
ioctl: arg1 == 0xc014ec00
brk: 1
pread64: 1
rt_sigprocmask: 1
lseek: 1
access: 1
arch_prctl: 1
set_tid_address: 1
set_robust_list: 1
rseq: 1
prlimit64: 1
munmap: 1
getrandom: arg2 in ~GRND_RANDOM
exit_group: 1
restart_syscall: 1
exit: 1
rt_sigreturn: 1
clock_getres: 1
clock_gettime: 1
gettimeofday: 1
