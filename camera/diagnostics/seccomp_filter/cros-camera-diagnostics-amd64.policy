getresuid: 1
getsockname: 1
gettid: 1
getuid: 1
# For DLC
ioctl: 1
lseek: 1
madvise: 1
memfd_create: 1
mmap: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
