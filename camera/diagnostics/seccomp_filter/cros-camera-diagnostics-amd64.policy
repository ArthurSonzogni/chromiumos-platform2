newfstatat: 1
openat: 1
pipe2: 1
poll: 1
pread64: 1
prlimit64: 1
# For DLC
prctl: 1
read: 1
recvmsg: 1
restart_syscall: 1
rt_sigaction: 1
rt_sigprocmask: 1
