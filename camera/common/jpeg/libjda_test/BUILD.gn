group("all") {
  deps = [ ":libjda_test" ]
}

executable("libjda_test") {
  sources = [ "//camera/common/jpeg/jpeg_decode_accelerator_test.cc" ]
  configs += [
    "//camera/build:cros_camera_common",
    "//common-mk:test",
  ]
  libs = [ "jpeg" ]
  pkg_deps = [
    "cros-camera-android-headers",
    "libbrillo",
    "libcros_camera",
    "libyuv",
  ]
}
