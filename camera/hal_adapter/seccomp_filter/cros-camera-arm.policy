recvmsg: 1
restart_syscall: 1
rt_sigaction: 1
rt_sigprocmask: 1
sched_getaffinity: 1
sched_getattr: 1
sched_getparam: 1
sched_getscheduler: 1
sched_setaffinity: 1
sched_setattr: 1
sched_setscheduler: 1
sched_yield: 1
send: 1
sendmsg: 1
set_robust_list: 1
set_tid_address: 1
setpriority: 1
setsockopt: 1
shutdown: 1
signalfd4: 1
# Allow PF_UNIX for syslog and mojo IPC.
# Allow PF_NETLINK with NETLINK_KOBJECT_UEVENT (15) for udev.
socket: arg0 == PF_UNIX || arg0 == PF_NETLINK && arg2 == 15
socketpair: 1
stat64: 1
statfs: 1
statx: 1
sysinfo: 1
tgkill: 1
uname: 1
unlink: 1
wait4: 1
write: 1
writev: 1
