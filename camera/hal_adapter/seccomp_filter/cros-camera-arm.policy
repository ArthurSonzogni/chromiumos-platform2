# rseq should be first due to frequency and purpose to decrease nptl latency
rseq: 1

_llseek: 1
_newselect: 1
accept4: 1
access: 1
bind: 1
brk: 1
clock_gettime: 1
clock_gettime64: 1
clock_nanosleep: 1
clock_nanosleep_time64: 1
clone: 1
close: 1
connect: 1
dup: 1
epoll_create1: 1
epoll_ctl: 1
epoll_wait: 1
eventfd2: 1
exit: 1
exit_group: 1
faccessat: 1
faccessat2: 1
fallocate: 1
fcntl64: 1
flock: 1
fstat64: 1
fstatat64: 1
fstatfs: 1
fstatfs64: 1
ftruncate64: 1
futex: 1
futex_time64: 1
getdents: 1
getdents64: 1
getegid32: 1
geteuid32: 1
getgid32: 1
getpid: 1
getpriority: 1
getrandom: arg2 in ~GRND_RANDOM
getsockname: 1
getsockopt: 1
gettid: 1
gettimeofday: 1
getuid32: 1
inotify_add_watch: 1
inotify_init: 1
inotify_rm_watch: 1
ioctl: 1
lstat64: 1
madvise: 1
memfd_create: 1
mkdir: 1
# Prevent PROT_WRITE | PROT_EXEC.
mmap2: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
mprotect: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
munmap: 1
name_to_handle_at: 1
nanosleep: 1
open: 1
openat: 1
pipe: 1
pipe2: 1
poll: 1
ppoll: 1
ppoll_time64: 1
prctl: 1
