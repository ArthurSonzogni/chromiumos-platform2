# rseq should be first due to frequency and purpose to decrease nptl latency
rseq: 1

brk: 1
clock_getres: 1
clock_gettime: 1
clock_nanosleep: 1
clone: 1
close: 1
connect: 1
epoll_create1: 1
epoll_ctl: 1
epoll_wait: 1
eventfd2: 1
exit: 1
exit_group: 1
fcntl: 1
flock: 1
fstat: 1
fstatfs: 1
futex: 1
newfstatat: 1
getdents64: 1
geteuid: 1
getpid: 1
getrandom: arg2 in ~GRND_RANDOM
getresgid: 1
getresuid: 1
getsockname: 1
gettid: 1
gettimeofday: 1
# For putting a socket in nonblocking mode
ioctl: arg1 == FIONBIO
lseek: 1
madvise: arg2 == MADV_DONTNEED
mmap: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
mprotect: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
munmap: 1
openat: 1
pidfd_open: 1
poll: 1
prctl: arg0 == PR_SET_NAME
prlimit64: 1
read: 1
recvfrom: 1
recvmsg: 1
restart_syscall: 1
rt_sigaction: 1
rt_sigprocmask: 1
rt_sigreturn: 1
sched_getaffinity: 1
sched_getattr: 1
sched_setattr: 1
sendmsg: 1
sendto: 1
set_robust_list: 1
sigaltstack: 1
socket: arg0 == AF_UNIX
statx: 1
tgkill: 1
timerfd_create: 1
timerfd_settime: 1
write: 1
