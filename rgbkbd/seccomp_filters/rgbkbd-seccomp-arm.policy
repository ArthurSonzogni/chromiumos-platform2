# rseq should be first due to frequency and purpose to decrease nptl latency
rseq: 1

access: 1
ARM_set_tls: 1
bind: 1
brk: 1
clock_getres: 1
clock_getres_time64: 1
clock_gettime: 1
clock_gettime64: 1
clock_nanosleep: 1
clock_nanosleep_time64: 1
clone: 1
close: 1
connect: 1
epoll_create1: 1
epoll_ctl: 1
epoll_wait: 1
execve: 1
exit: 1
exit_group: 1
faccessat: 1
faccessat2: 1
fcntl64: 1
fstatat64: 1
fstatfs: 1
futex_time64: 1
getdents64: 1
getegid32: 1
geteuid32: 1
getgid32: 1
getpid: 1
getrandom: 1
getresgid32: 1
getresuid32: 1
getsockname: 1
gettid: 1
gettimeofday: 1
getuid32: 1
# CROS_EC_DEV_IOCXCMD: 0xc0283a00
# CROS_EC_DEV_IOCXCMD_V2  = 0xc014ec00
ioctl: arg1 == 0xc0283a00 || arg1 == TCGETS || arg1 == 0xc014ec00
_llseek: 1
madvise: 1
mmap2: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
mprotect: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
munmap: 1
name_to_handle_at: 1
openat: 1
pipe2: 1
poll: 1
read: 1
readlink: 1
readlinkat: 1
recvmsg: 1
restart_syscall: 1
rt_sigaction: 1
rt_sigprocmask: 1
rt_sigreturn: 1
send: 1
sendmsg: 1
set_robust_list: 1
setsockopt: 1
set_tid_address: 1
signalfd4: 1
socket: arg0 == AF_UNIX || arg0 == AF_NETLINK
statx: 1
timerfd_create: 1
ugetrlimit: 1
uname: 1
write: 1
