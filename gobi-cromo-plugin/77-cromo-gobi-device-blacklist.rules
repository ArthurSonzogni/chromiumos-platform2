# do not edit this file, it will be overwritten on update

ACTION!="add|change", GOTO="mm_cromo_gobi_device_blacklist_end"
SUBSYSTEM!="usb", GOTO="mm_cromo_gobi_device_blacklist_end"
ENV{DEVTYPE}!="usb_device",  GOTO="mm_cromo_gobi_device_blacklist_end"
ATTRS{idUsbDriver}!="QCSerial2k", GOTO="mm_cromo_gobi_device_blacklist_end"

# Make sure Modem Manager Ignores all devices handled by QCSerial2k
ENV{ID_MM_DEVICE_IGNORE}="1"

