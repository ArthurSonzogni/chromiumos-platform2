access: 1
arch_prctl: 1
brk: 1
clock_getres: 1
clock_gettime: 1
clock_nanosleep: 1
# used to create a thread reading IIO devices' samples
clone: 1
close: 1
connect: 1
creat: 1
epoll_create1: 1
epoll_ctl: 1
epoll_wait: 1
eventfd2: 1
exit: 1
exit_group: 1
fcntl: 1
flock: 1
fstat: 1
fstatfs: 1
futex: 1
getdents: 1
getdents64: 1
getegid: 1
geteuid: 1
getgid: 1
getpid: 1
getrandom: 1
getresgid: 1
getresuid: 1
getsockname: 1
gettid: 1
gettimeofday: 1
getuid: 1
# used in libiio as BLOCK_FREE_IOCTL: _IO('i', 0xa1)
ioctl: arg1 == 0x69a1
lseek: 1
madvise: 1
memfd_create: 1
mmap: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
mprotect: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
munmap: 1
nanosleep: 1
newfstatat: 1
openat: 1
pipe2: 1
poll: 1
prctl: arg0 == PR_SET_NAME
read: 1
recvmsg: 1
restart_syscall: 1
rt_sigaction: 1
rt_sigprocmask: 1
rt_sigreturn: 1
sendmsg: 1
sendto: 1
setpriority: 1
set_robust_list: 1
set_tid_address: 1
signalfd4: 1
# Allow AF_UNIX for syslog and mojo IPC.
socket: arg0 == AF_UNIX
stat: 1
tgkill: 1
uname: 1
write: 1
