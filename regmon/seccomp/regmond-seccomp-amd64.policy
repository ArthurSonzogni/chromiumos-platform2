ioctl: arg1 == TCGETS || arg1 == SIOCGIFFLAGS
openat: 1
# Required for DBus connection
socket: arg0 == AF_UNIX
gettid: 1
epoll_wait: 1
poll: 1
recvmsg: 1
geteuid: 1
epoll_ctl: 1
sendmsg: 1
getpid: 1
connect: 1
getuid: 1
getgid: 1
getegid: 1
rt_sigprocmask: 1
signalfd4: 1
sendto: 1
read: 1
close: 1
pipe2: 1
fcntl: 1
write: 1
epoll_create1: 1
getresuid: 1
getresgid: 1
getsockname: 1
restart_syscall: 1
exit: 1
exit_group: 1
rt_sigreturn: 1
newfstatat: 1
uname: 1
getrandom: arg2 in ~GRND_RANDOM
flock: 1
eventfd2: 1
# Don't allow mmap with both PROT_WRITE and PROT_EXEC.
mmap: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
