# These seccomp rules specify the syscalls used by the override_max_pressure.

# ioctl: arg1 == EVIOCGABS || EVIOCSABS
ioctl: arg1 == 0x80184558 || arg1 == 0x401845d8
openat: 1
getpid: 1

# Basic process functionality.
restart_syscall: 1
exit: 1
exit_group: 1
rt_sigreturn: 1
