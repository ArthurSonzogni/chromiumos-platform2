<?xml version="1.0" encoding="UTF-8" ?>

<node name="/org/chromium/authpolicy/Domain"
      xmlns:tp="http://telepathy.freedesktop.org/wiki/DbusSpec#extensions-v0">
  <interface name="org.chromium.authpolicy.Domain">
    <method name="Auth">
      <arg name="login" type="s" direction="in"/>
      <arg name="password" type="h" direction="in"/>
      <arg name="code" type="i" direction="out"/>
      <arg name="sid" type="s" direction="out" />
      <annotation name="org.chromium.DBus.Method.Kind" value="normal"/>
    </method>
    <method name="Join">
      <arg name="machine_name" type="s" direction="in"/>
      <arg name="login" type="s" direction="in"/>
      <arg name="password" type="h" direction="in"/>
      <arg name="code" type="i" direction="out"/>
      <annotation name="org.chromium.DBus.Method.Kind" value="normal"/>
    </method>
  </interface>
</node>
