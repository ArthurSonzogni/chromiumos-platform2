# rseq should be first due to frequency and purpose to decrease nptl latency
rseq: 1

close: 1
exit: 1
exit_group: 1
fcntl: 1
fstat: 1
newfstatat: 1
futex: 1
getrandom: arg2 in ~GRND_RANDOM
getuid: 1
lseek: 1
open: 1
openat: 1
read: 1
restart_syscall: 1
rt_sigreturn: 1
stat: 1
write: 1
getpid: 1
gettimeofday: 1
pipe2: 1
