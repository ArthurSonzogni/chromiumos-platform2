rseq: 1
epoll_wait: 1
sendto: 1
recvmsg: 1
read: 1
futex: 1
gettid: 1
getrandom: arg2 in ~GRND_RANDOM
poll: 1
rt_sigprocmask: 1
fcntl: 1
openat: 1
write: 1
epoll_ctl: 1
geteuid: 1
getuid: 1
getgid: 1
getegid: 1
signalfd4: 1
getpid: 1
sendmsg: 1
pipe2: 1
uname: 1
munmap: 1
newfstatat: 1
epoll_create1: 1
getresuid: 1
getresgid: 1
connect: 1
getsockname: 1
rt_sigaction: 1
clock_getres: 1
clone: 1
