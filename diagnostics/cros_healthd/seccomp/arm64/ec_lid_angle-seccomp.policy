# rseq should be first due to frequency and purpose to decrease nptl latency
rseq: 1

openat: 1
read: 1
brk: 1
uname: 1
write: 1
exit_group: 1
newfstatat: 1

# CROS_EC_DEV_IOCRDMEM_V2 = 0xc108ec01
# CROS_EC_DEV_IOCXCMD = 0xc0283a00
# CROS_EC_DEV_IOCXCMD_V2 = 0xc014ec0
ioctl: arg1 == 0xc108ec01 || arg1 == 0xc0283a00 || arg1 == 0xc014ec00

clock_getres: 1
clock_gettime: 1
gettimeofday: 1
gettid: 1
getuid: 1
geteuid: 1
getgid: 1
getegid: 1
epoll_create1: 1
pipe2: 1
epoll_ctl: 1
getrandom: arg2 in ~GRND_RANDOM
fcntl: 1
recvmsg: 1
rt_sigprocmask: 1
signalfd4: 1
getpid: 1
memfd_create: 1
eventfd2: 1
epoll_pwait: 1
sendto: 1
close: 1
socket: arg0 == AF_UNIX
connect: 1
mmap: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
process_vm_readv: 1
