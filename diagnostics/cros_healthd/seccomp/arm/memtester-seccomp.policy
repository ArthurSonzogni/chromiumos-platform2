# rseq should be first due to frequency and purpose to decrease nptl latency
rseq: 1

exit_group: 1
exit: 1
fstat64: 1
fstatat64: 1
mlock: 1
mmap2: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
munlock: 1
restart_syscall: 1
rt_sigreturn: 1
statx: 1
write: 1
clock_getres: 1
clock_getres_time64: 1
clock_gettime: 1
clock_gettime64: 1
gettimeofday: 1
