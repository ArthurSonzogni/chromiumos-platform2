# Socket is a required syscall for delegate process.
socket: arg0 == AF_UNIX
socketpair: arg0 == AF_UNIX

clock_getres: 1
clock_gettime: 1
gettimeofday: 1
connect: 1
exit_group: 1
clock_getres_time64: 1
clock_gettime64: 1

epoll_create1: 1
epoll_ctl: 1
epoll_wait: 1
eventfd2: 1
fcntl64: 1
getegid32: 1
geteuid32: 1
getgid32: 1
getpid: 1
getrandom: arg2 in ~GRND_RANDOM
gettid: 1
getuid32: 1
memfd_create: 1
pipe2: 1
read: 1
recvmsg: 1
rt_sigprocmask: 1
send: 1
sendmsg: 1
signalfd4: 1
uname: 1
write: 1
mmap2: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
mseal: 1
close: 1
openat: 1
pread64: 1
futex: 1
munmap: 1
