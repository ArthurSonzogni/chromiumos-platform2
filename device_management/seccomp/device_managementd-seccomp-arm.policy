# rseq should be first due to frequency and purpose to decrease nptl latency
rseq: 1

access: 1
brk: 1
clock_getres: 1
clock_getres_time64: 1
clock_gettime: 1
clock_gettime64: 1
clock_nanosleep: 1
clone: 1
close: 1
connect: 1
epoll_create: 1
epoll_create1: 1
epoll_ctl: 1
epoll_wait: 1
exit_group: 1
exit: 1
faccessat: 1
faccessat2: 1
fcntl64: 1
fdatasync: 1
flock: 1
fstat: 1
fstat64: 1
fsync: 1
futex: 1
getegid32: 1
geteuid32: 1
getgid32: 1
getpid: 1
getrandom: 1
getresgid32: 1
getresuid32: 1
getsockname: 1
gettid: 1
gettimeofday: 1
getuid32: 1
lseek: 1
madvise: 1
mkdir: 1
mlock: 1
mmap2: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
mprotect: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
munlock: 1
munmap: 1
nanosleep: 1
open: 1
openat: 1
pipe2: 1
poll: 1
prctl: arg0 == PR_SET_NAME
prlimit64: arg2 == 0 && arg3 != 0
read: 1
recvmsg: 1
rename: 1
restart_syscall: 1
rt_sigaction: 1
rt_sigprocmask: 1
rt_sigreturn: 1
sched_getattr: 1
sched_setattr: 1
send: 1
sendmsg: 1
sendto: 1
set_robust_list: 1
set_tid_address: 1
setgroups32: 1
setpriority: 1
setresgid32: 1
setuid32: 1
signalfd4: 1
socket: arg0 == AF_UNIX
stat: 1
stat64: 1
statx: 1
sysinfo: 1
uname: 1
unlinkat: 1
write: 1

# tgkill is used by abort() and CHECK()/LOG(FATAL).
fstatfs: 1
fstatfs64: 1
tgkill: arg2 == SIGABRT

process_vm_readv: 1
