define(`dir_unconfined', `$1/.* u:object_r:cros_unconfined_exec:s0')
define(`file_unconfined', `$1 u:object_r:cros_unconfined_exec:s0')
dir_unconfined(/bin)
dir_unconfined(/sbin)
dir_unconfined(/usr/bin)
dir_unconfined(/usr/sbin)
file_unconfined(/usr/libexec/sudo/libsudo_util.*)
dir_unconfined(/usr/local/bin)
file_unconfined(/usr/local/lib/libpython.*)
dir_unconfined(/usr/local/lib/python.*/lib-dynload)

dir_unconfined(/usr/share/userfeedback/scripts)
dir_unconfined(/opt/google/touch/scripts)
