prctl: arg0 == PR_CAPBSET_READ || arg0 == PR_SET_SECUREBITS || \
       arg0 == PR_SET_SECCOMP || arg0 == PR_CAPBSET_DROP
mmap2: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
close: 1
mprotect: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
socket: arg0 == AF_LOCAL || arg0 == AF_INET || arg0 == AF_INET6 || \
        arg0 == AF_NETLINK
getsockopt: 1
fcntl64: 1
statx: 1
munmap: 1
openat: 1
read: 1
fstatfs64: 1
gettid: 1
capset: 1
capget: 1
setsockopt: 1
flock: 1
exit_group: 1
setgroups32: 1
setresgid32: 1
setresuid32: 1
_llseek: 1
write: 1
connect: 1
getsockname: 1
recvmsg: 1
statfs64: 1
futex: 1
bind: 1
sendto: 1
poll: 1
clock_gettime: 1
clock_gettime64: 1
ioctl: arg1 == TCGETS || arg1 == FIONREAD
send: 1
restart_syscall: 1
exit: 1
rt_sigreturn: 1
brk: 1
recvfrom: 1
uname: 1
sysinfo: 1
sendmmsg: 1
writev: 1
